[user]
	name = Austin M. Matherne
	email = AustinMatherne@Gmail.com
	signingkey = 37F543A5
[core]
	editor = vim
	autocrlf = input
[merge]
	tool = vimdiff
[color]
	ui = auto
[alias]
	s = status -s
	a = add
	p = push
	pt = push --tag
	pa = push --recurse-submodules=on-demand
	pl = pull
	co = checkout
	b = branch
	cm = commit -S
	un = reset HEAD --
	rs = reset --hard
	last = log -1 HEAD
	history = log -p --graph
	lg = log --oneline --decorate --all --graph
	unstash = !git stash show -p | git apply -R
[commit]
	template = /home/austinmatherne/.gitmessage
[help]
	autocorrect = 1
[github]
	user = AustinMatherne
[push]
	default = simple
[pull]
	rebase = true
[rerere]
	enabled = true
